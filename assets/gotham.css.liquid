@font-face {
  font-family: Gotham;
  src: url('{{ 'Gotham.eot' | asset_url }}');
  src: url('{{ 'Gotham.woff2' | asset_url }}') format("woff2"),
     url('{{ 'Gotham.woff' | asset_url }}') format("woff"),
     url('{{ 'Gotham.ttf' | asset_url }}') format("truetype"),
     url('{{ 'Gotham.svg' | asset_url }}#Gotham-Light') format("svg"),
     url('{{ 'Gotham.eot' | asset_url }}?#iefix') format("embedded-opentype");
  font-weight: 100;
  font-style: normal
}

