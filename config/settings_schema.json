[
  {
    "name": "SEO - Page Titles",
    "settings": [
      {
        "type": "paragraph",
        "content": "Powered by Plug in SEO Plus"
      },
      {
        "type": "paragraph",
        "content": "Page titles are displayed in the web browser tab and as the blue underlined link text in search engines"
      },
      {
        "type": "paragraph",
        "content": "[Page titles help](https:\/\/help.pluginuseful.com\/hc\/en-us\/articles\/209539509-Page-titles)"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleTruncateChars",
        "label": "Maximum length (characters)",
        "default": "999"
      },
      {
        "type": "header",
        "content": "Appender"
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleEnableAppender",
        "label": "Append to short page titles?",
        "default": true
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleAppenderTemplate",
        "label": "Append",
        "default": "| %% shop.name %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleAppenderTargetLength",
        "label": "Target length",
        "default": "70"
      },
      {
        "type": "header",
        "content": "Page Title Templates"
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTemplateApplyToAll",
        "label": "Also apply templates to manually entered page titles?",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTruncateEnableForCollections",
        "label": "Apply To Collections",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTruncateEnableForProducts",
        "label": "Apply To Products",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTruncateEnableForPages",
        "label": "Apply To Pages",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTruncateEnableForArticles",
        "label": "Apply To Articles",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_pageTitleTruncateEnableForBlogs",
        "label": "Apply To Blogs",
        "default": true
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleDefaultTemplate",
        "label": "Default",
        "default": "%% page_title %%",
        "info": "this template will apply to all blank page types below"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleIndexTemplate",
        "label": "Homepage"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitlePageTemplate",
        "label": "Page",
        "default": "%% page_title %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleSearchTemplate",
        "label": "Search",
        "default": "Shop %% search.terms %% at %% shop.name %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleProductTemplate",
        "label": "Product",
        "default": "%% product.title %%%% product.selected_variant.title || prepend_not_empty: ' ' %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleCollectionTemplate",
        "label": "Collection",
        "default": "Shop %% current_tags || join: ', ' || append_not_empty: ' ' %%%% collection.title || escape %% at %% shop.name %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleCatalogTemplate",
        "label": "Catalog Page",
        "default": "Browse %% collection.products || map: 'collections' || map: 'title' || uniq || take: '5' || join: ', ' || escape || remove: 'Home page, ' || remove: 'home page, '  || remove: 'Home page' || remove: 'Home page'  %% at %% shop.name %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleBlogTemplate",
        "label": "Blog",
        "default": "%% shop.name %% %% blog.title %%%% blog.all_tags || take: '5' || join: ', ' || prepend_not_empty: ' blog: ' || append_not_empty: ' and more' %%"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitleArticleTemplate",
        "label": "Article",
        "default": "%% article.title %%%% article.tags || take: '3' || join: ', ' || prepend_not_empty: ' | ' || append_not_empty: ' and more' %% | %% shop.name %% %% blog.title %% blog"
      },
      {
        "type": "text",
        "id": "pluginseo_pageTitle404Template",
        "label": "404",
        "default": "404: Page Not Found - %% shop.name %%"
      }
    ]
  },
  {
    "name": "SEO - Meta Descriptions",
    "settings": [
      {
        "type": "paragraph",
        "content": "Powered by Plug in SEO Plus"
      },
      {
        "type": "paragraph",
        "content": "Meta descriptions are displayed as the paragraph text in search engines"
      },
      {
        "type": "paragraph",
        "content": "[Meta descriptions help](https:\/\/help.pluginuseful.com\/hc\/en-us\/articles\/209542049-Meta-descriptions)"
      },
      {
        "type": "text",
        "id": "pluginseo_metaDescriptionTruncateChars",
        "label": "Maximum length (characters)",
        "default": "170"
      },
      {
        "type": "header",
        "content": "Meta Description Templates"
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTemplateApplyToAll",
        "label": "Also apply templates to manually entered meta descriptions?",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTruncateEnableForCollections",
        "label": "Apply To Collections",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTruncateEnableForProducts",
        "label": "Apply To Products",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTruncateEnableForPages",
        "label": "Apply To Pages",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTruncateEnableForArticles",
        "label": "Apply To Articles",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "pluginseo_metaDescriptionTruncateEnableForBlogs",
        "label": "Apply To Blogs",
        "default": true
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionDefaultTemplate",
        "label": "Default",
        "default": "%% page_description %%",
        "info": "this template will apply to all blank page types below"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionIndexTemplate",
        "label": "Homepage"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionPageTemplate",
        "label": "Page",
        "default": "%% page.content || strip_newlines || strip_html || escape || append_not_empty: '.' %% Visit %% shop.name %%"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionSearchTemplate",
        "label": "Search",
        "default": "Find %% search.terms || default: 'it' %% here%% search.results || map: 'title' || take: '3' || join: ', ' || prepend_not_empty: ' including ' || append_not_empty: ' and more' %% at %% shop.name %%"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionProductTemplate",
        "label": "Product",
        "default": "%% product.description || strip_newlines || replace: '.,', ',' || strip_html || escape %%"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionCollectionTemplate",
        "label": "Collection",
        "default": "%% collection.all_tags || take: '3' || join: ', ' || prepend_not_empty: 'Shop ' || append_not_empty: ' and more ' %%%% collection.title || escape %% at %% shop.name %% %% collection.description || strip_html || strip_newlines || escape || strip %%"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionCatalogTemplate",
        "label": "Catalog Page",
        "default": "Discover %% collection.products || map: 'title' || uniq || take: '3' || join: ', ' || escape %% and more."
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionBlogTemplate",
        "label": "Blog",
        "default": "%% blog.articles || map: 'title' || take: '3' || join: ', ' || prepend_not_empty: 'Read about ' || append_not_empty: ' and more on the '%%%% shop.name %% %% blog.title %% blog."
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescriptionArticleTemplate",
        "label": "Article",
        "default": "%% article.excerpt_or_content || strip_newlines || strip_html || escape %%%% article.tags || take: '6' || join: ', ' || prepend_not_empty: ' Read about ' || append_not_empty: ' and more' %% %% article.title %% on the %% shop.name %% %% blog.title %% blog"
      },
      {
        "type": "textarea",
        "id": "pluginseo_metaDescription404Template",
        "label": "404",
        "default": "The page you requested can't be found, please visit %% shop.domain %% instead"
      }
    ]
  },
