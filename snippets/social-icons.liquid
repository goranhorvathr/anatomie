<ul class="social-links">
  {% if settings.social_twitter_url != blank %}
    <li><a title="Twitter" class="social-link twitter" target="_blank" aria-label="Twitter" href="{{ settings.social_twitter_url }}">{% include 'icon-twitter' %}</a></li>
  {% endif %}
  {% if settings.social_facebook_url != blank %}
    <li><a title="Facebook" class="social-link facebook" target="_blank" aria-label="Facebook" href="{{ settings.social_facebook_url }}">{% include 'icon-facebook' %}</a></li>
  {% endif %}
  {% if settings.social_youtube_url != blank %}
    <li><a title="Youtube" class="social-link youtube" target="_blank" aria-label="Youtube" href="{{ settings.social_youtube_url }}">{% include 'icon-youtube' %}</a></li>
  {% endif %}
  {% if settings.social_vimeo_url != blank %}
    <li><a title="Vimeo" class="social-link vimeo" target="_blank" aria-label="Vimeo" href="{{ settings.social_vimeo_url }}">{% include 'icon-vimeo' %}</a></li>
  {% endif %}
  {% if settings.social_instagram_url != blank %}
    <li><a title="Instagram" class="social-link instagram" target="_blank" aria-label="Instagram" href="{{ settings.social_instagram_url }}">{% include 'icon-instagram' %}</a></li>
  {% endif %}
  {% if settings.social_pinterest_url != blank %}
    <li><a title="Pinterest" class="social-link pinterest" target="_blank" aria-label="Pinterest" href="{{ settings.social_pinterest_url }}">{% include 'icon-pinterest' %}</a></li>
  {% endif %}
  {% if settings.catalog_url != blank %}
    <li><a title="Catalog" class="social-link catalog" aria-label="Catalog" href="{{ settings.catalog_url }}">{% include 'icon-catalog', class: 'icon catalog_small' %}</a></li>
  {% endif %}
  {% if settings.enable_facetime_icon %}<li><a class="social-link" href="#">{% include 'icon-facetime', class: 'icon facetime_small' %}</a></li>{% endif %}


  {% if settings.social_custom_url != blank %}
    {% capture custom_icon_size %}{% if settings.soc_icons_double %}x46{% else %}x30{% endif %}{% endcapture %}
    <li class="social-custom">
      <a title="{{ settings.social_custom_icon.alt }}" class="social-custom__main" target="_blank" href="{{ settings.social_custom_url }}">
        {{ settings.social_custom_icon | img_url: custom_icon_size | img_tag: settings.social_custom_icon.alt }}
      </a>
    </li>
  {% endif %}
</ul>
